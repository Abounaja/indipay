    "license": "MIT",
    "keywords": [
        "Laravel",
        "Payment Gateways",
        "CCAvenue",
        "PayUMoney",
        "EBS",
        "CitrusPay",
        "InstaMojo",
        "Mobikwik / ZapakPay",
        "Paytm",
        "Mocker"
    ],
    "authors": [
        {
            "name": "Shiburaj",
            "email": "powerupneo@gmail.com"
        },
        {
            "name": "Oussama",
            "email": "oussama@coderflex.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/pint": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Abounaja\\Indipay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abounaja\\Indipay\\IndipayServiceProvider"
            ],
            "aliases": {
                "Indipay": "Abounaja\\Indipay\\Facades\\Indipay"
            }
        }
    },
    "minimum-stability": "dev"
